{
  "author": "R Brewster <brewster1134+atrackt@gmail.com>",
  "bugs": "https://github.com/atrackt/atrackt/issues",
  "description": "A simple developer API to uniformly implement analytics services",
  "homepage": "https://github.com/atrackt/atrackt",
  "license": "SEE LICENSE IN LICENSE",
  "name": "atrackt",
  "private": true,
  "devDependencies": {
    "@babel/preset-env": "^7",
    "@babel/preset-typescript": "^7",
    "@commitlint/cli": "^11",
    "@commitlint/config-conventional": "^11",
    "@types/jest": "^26",
    "@typescript-eslint/parser": "^4",
    "concurrently": "^5",
    "css-loader": "^4",
    "deepmerge": "^4",
    "http-server": "^0",
    "husky": "^4",
    "jest": "^26",
    "lerna": "^3",
    "reload": "^3",
    "sass": "^1",
    "sass-loader": "^10",
    "style-loader": "^1",
    "systematize": "^3",
    "ts-loader": "^8",
    "typescript": "^4",
    "webpack": "^4",
    "webpack-cli": "^3"
  },
  "engines": {
    "node": ">=13.2.0"
  },
  "keywords": [
    "adobe",
    "omniture",
    "sitecatalyst",
    "analytics",
    "jquery",
    "api",
    "developer",
    "dom",
    "event",
    "library",
    "handler",
    "platform",
    "services",
    "tool",
    "tracking"
  ],
  "publishConfig": {
    "access": "public",
    "directory": ".dist",
    "registry": "https://registry.npmjs.org",
    "scope": "@atrackt"
  },
  "repository": {
    "type": "git",
    "url": "github:atrackt/atrackt"
  },
  "scripts": {
    "build": "lerna run prepare",
    "ci-test": "jest --ci",
    "demo-browser": "reload --start-page demo/index.html",
    "demo-server": "http-server .",
    "demo-ts": "webpack --watch --config webpack.demo.js",
    "demo": "concurrently --kill-others \"yarn:demo-*\"",
    "postpublish": "yarn reset",
    "prepublish": "",
    "release": "lerna publish --create-release github",
    "reset": "git clean -dfX && yarn",
    "test": "jest --watch"
  },
  "workspaces": [
    "core",
    "handlers/*",
    "services/*"
  ]
}
