{
  "author": "R Brewster <brewster1134+atrackt@gmail.com>",
  "bugs": "https://github.com/atrackt/atrackt/issues",
  "description": "A simple developer API to uniformly implement analytics services",
  "homepage": "https://github.com/atrackt/atrackt",
  "license": "SEE LICENSE IN LICENSE",
  "name": "atrackt",
  "private": true,
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7",
    "@babel/preset-typescript": "^7",
    "@types/jest": "^26",
    "deepmerge": "^4",
    "jest": "^26",
    "lerna": "^3",
    "typescript": "^4"
  },
  "engines": {
    "node": ">=13.2.0"
  },
  "keywords": [
    "adobe",
    "omniture",
    "sitecatalyst",
    "analytics",
    "jquery",
    "api",
    "developer",
    "dom",
    "event",
    "library",
    "handler",
    "platform",
    "services",
    "tool",
    "tracking"
  ],
  "publishConfig": {
    "access": "public",
    "directory": ".dist",
    "registry": "https://npm.pkg.github.com",
    "scope": "@atrackt"
  },
  "repository": {
    "type": "git",
    "url": "github:atrackt/atrackt"
  },
  "scripts": {
    "build": "lerna run prepare",
    "demo": "webpack-dev-server --config webpack.demo.coffee",
    "prepublish": "tsc",
    "postpublish": "yarn reset",
    "release": "lerna publish --create-release github",
    "reset": "git clean -dfx && yarn",
    "test": "jest"
  },
  "workspaces": [
    "core",
    "handlers/*",
    "services/*"
  ]
}
